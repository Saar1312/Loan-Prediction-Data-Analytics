for (f in files){
read.csv(f, sep=";", na.strings=empty_values)
}
}
countNa <- function(df)
{
for(col in names(df)){
cat(col," ",sum(is.na(df[[col]])),"\n")
}
}
checkNa <- function(frames)
{
for(f in frames){
countNa(f)
cat("#################################\n")
}
}
